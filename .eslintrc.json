{
   "env": {
      "es2021": true,
      "node": true
   },
   "extends": [
      "./node_modules/standard/eslintrc.json"
   ],
   "overrides": [],
   "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
   },
   "plugins": ["prettier"],
   "rules": {
      "camelcase": "off",
      "multiline-ternary": "off",
      "no-unused-vars": "warn",
      "operator-linebreak": "off",
      "prettier/prettier": "warn"
   }
}